{
    "name": "jalallinux/cayenne-lpp",
    "description": "PHP Cayenne LPP encoder and decoder",
    "keywords": [
        "JalalLinuX",
        "cayenne-lpp"
    ],
    "homepage": "https://github.com/jalallinux/cayenne-lpp",
    "license": "MIT",
    "authors": [
        {
            "name": "JalalLinuX",
            "email": "smjjalalzadeh93@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "JalalLinuX\\CayenneLpp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JalalLinuX\\CayenneLpp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
